<%= form_for @sponsor, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :name, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :name, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :status, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :status, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :type, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :type, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :orgnization_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :orgnization_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :gender, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :gender, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :address, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :address, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :country, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :country, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :partnership_start_date, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :partnership_start_date, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :email, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :email, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :contact_1, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :contact_1, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :contact_2, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :contact_2, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :additional_info, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :additional_info, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :osra_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :osra_id, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                sponsor_path, :class => 'btn' %>
  </div>
<% end %>
