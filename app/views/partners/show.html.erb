<%- model_class = Partner -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @partner.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:governante) %>:</strong></dt>
  <% #debugger %>
  <dd><%= t_governante @partner.governante %></dd>
  <dt><strong><%= model_class.human_attribute_name(:region) %>:</strong></dt>
  <dd><%= @partner.region %></dd>
  <dt><strong><%= model_class.human_attribute_name(:partnership_start_date) %>:</strong></dt>
  <dd><%= @partner.partnership_start_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status) %>:</strong></dt>
  <dd><%= t "statuses.#{@partner.status}", :default => @partner.status %></dd>
  <dt><strong><%= model_class.human_attribute_name(:osra_id) %>:</strong></dt>
  <dd><%= @partner.osra_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              partners_path, :class => 'btn'  %>
  <%= link_to t('.upload', :default => t("helpers.links.upload")),
              upload_list_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_partner_path(@partner), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              partner_path(@partner),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
