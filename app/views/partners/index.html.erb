<%- model_class = Partner -%>
<div class="page-header">
  <h1><%= t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
  <tr>
    <th><%= model_class.human_attribute_name(:osra_id) %></th>
    <th><%= model_class.human_attribute_name(:name) %></th>
    <th><%= model_class.human_attribute_name(:governorate) %></th>
    <th><%= model_class.human_attribute_name(:region) %></th>
    <th><%= model_class.human_attribute_name(:partnership_start_date) %></th>
    <th><%= model_class.human_attribute_name(:status) %></th>
    <th><%= t '.actions', :default => t("helpers.actions") %></th>
  </tr>
  </thead>
  <tbody>
  <% @partners.each do |partner| %>
      <tr>
        <td><%= link_to partner.osra_id, partner_path(partner) %></td>
        <td><%= partner.name %></td>
        <td><%= t_option 'governorates', partner.governorate %></td>
        <td><%= partner.region %></td>
        <td><%= partner.partnership_start_date %></td>
        <td><%= t_option 'statuses', partner.status %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_partner_path(partner), :class => 'btn btn-mini' %>
        </td>
      </tr>
  <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_partner_path,
            :class => 'btn btn-primary' %>
